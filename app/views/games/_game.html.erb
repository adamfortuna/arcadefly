<li class="<%= cycle("even","odd")%>">
<% if role?('game_editor') -%>
<ul class="left-shift admin">
  <li><%= link_to 'Edit', edit_game_path(game), :class => 'admin' %></li>
  <li><%= link_to 'Destroy', game_path(game), :class => 'admin', :confirm => 'Are you sure?', :method => :delete %></li>
</ul>
<% end %>

	<%= link_to(game.name, game_path(game)) %>
	<span class="count"><%= pluralize(game.playables_count, "arcade") %></span>
  <span class="index" style="width: <%= (game.playables_count/@playables_count)*100 %>%">(<%= ((game.playables_count/@playables_count)*100) %>%)</span>
</li>