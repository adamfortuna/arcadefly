<% title "Popular Arcades" -%>

<%= render :partial => 'popular/menu' %>

<% tabbed_box do %>
  <h2><%= @title %></h2>
  <ul class="data">
    <%= render :partial => 'arcades/arcades', :locals => { :arcades => @arcades, :map => @map } %>
  </ul>
  <p>
    <%= will_paginate @arcades %>
  </p>
<% end %>